<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.terminal-to-ps</string>

    <key>ProgramArguments</key>
    <array>
        <string>REPLACE_WITH_PATH/target/release/terminal-to-ps</string>
    </array>

    <key>WorkingDirectory</key>
    <string>REPLACE_WITH_PATH</string>

    <key>RunAtLoad</key>
    <true/>

    <key>KeepAlive</key>
    <true/>

    <key>StandardOutPath</key>
    <string>REPLACE_WITH_PATH/server.log</string>

    <key>StandardErrorPath</key>
    <string>REPLACE_WITH_PATH/server.error.log</string>
</dict>
</plist>
